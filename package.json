{
  "name": "website",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "node layout/generateMarkup.js && react-scripts start",
    "build": "node layout/generateMarkup.js && react-scripts build && react-snapshot",
    "clean": "eslint --fix --ext js src",
    "deploy": "aws s3 cp build s3://spencerrc-website/ --recursive",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "dependencies": {
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-router-dom": "^5.0.0",
    "react-scripts": "2.1.8"
  },
  "devDependencies": {
    "react-snapshot": "^1.3.0"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "reactSnapshot": {
    "include": [
      "/404"
    ]
  }
}
